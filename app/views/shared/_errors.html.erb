<div class="row">
  <div class="col-xs-8 col-xs-offset-2">
    <div class="panel panel-danger">
      <div class="panel-header">
        <h2 class="panel-title"><%=pluralize(obj.errors.size, "error") %> </h2>
      </div>
      <div class = "panel-body" >
        <ul>
        <% obj.errors.full_messages.each do |error| %>
          <li>
            <%= error %>
          </li>
        <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>